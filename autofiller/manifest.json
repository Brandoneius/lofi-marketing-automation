{
    "manifest_version": 3,
    "name": "Custom Auto-Filler",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "48": "icon.png"
    }
}
